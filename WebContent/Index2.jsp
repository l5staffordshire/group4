<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Update Staff Profile</title>
	<link href="css/bootstrap.min.css" rel="stylesheet">
     <script src="js/html5shiv.min.js"></script>
     <script src="js/respond.min.js"></script>
</head>
<body>
	<div class="comtainer">
		<div class="panel panel-primary">
			<div class="panel-heading"><center><strong><h4>Update Staff Profile</h4></strong></center></div>
			<table class="table table-striped tables-hover table-bordered table-condensed">
				<tr>
					<th class="col-lg-3">Name</th>
					<td><input type="text" name="" class="form-control"></td>
					<td><input type="text" name="" class="form-control"></td>
					<td><input type="text" name="" class="form-control"></td>
				</tr>
				
				<tr>
					<th class="col-lg-3">Date Of Birth</th>
					<td colspan="3"><input type="date" name="" class="form-control"></td>
				
				</tr>
				
				<tr>
					<th class="col-lg-3">Gender</th>
					<td colspan ="3"><input type="text" name="" class="form-control"></td>
					
				</tr>
				
				<tr>
					<th class="col-lg-3">Department</th>
					<td colspan="3"><input type="text" name="" class="form-control"></td>
					
				</tr>
				
				<tr>
					<th class="col-lg-3">Rank</th>
					<td colspan="3"><input type="text" name="" class="form-control"></td>
					
				</tr>
				
				<tr>
					<th class="col-lg-3">Phone</th>
					<td colspan="1.5"><input type="number" name="" class="form-control"></td>
					<td colspan="1.5"><input type="number" name="" class="form-control"></td>
					
				</tr>
				
				<tr>
					<th class="col-lg-3">Email</th>
					<td colspan="3"><input type="email" name="" class="form-control"></td>
					
				</tr>
				
				<tr>
					<th class="col-lg-3">Residential Address</th>
					<td colspan="3"><input type="text" name="" class="form-control"></td>
				
				</tr>
				
				<tr>
					<th class="col-lg-3">Postal Address</th>
					<td colspan="3"><input type="text" name="" class="form-control"></td>
					
				</tr>
		
			</table>
		</div>
			<div>
				<center>
					<input type="submit"  value="Update" name="submit" class="btn btn-primary">
				</center>
				 
			</div>
	</div>
	<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.11.3/jquery.min.js"></script>
 <script src="js/bootstrap.min.js"></script>

</body>
</html>